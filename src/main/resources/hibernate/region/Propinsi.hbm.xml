<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.integrasolusi.pusda.sisda.persistence.region.Propinsi" table="propinsi">
        <id name="id" column="UID" type="java.lang.Long" unsaved-value="null">
            <generator class="native"/>
        </id>

        <version name="version" access="field" column="OBJ_VERSION"/>

        <property name="name" type="java.lang.String" column="NAME"/>

        <property name="shortName" type="java.lang.String" column="SHORT_NAME"/>
    </class>
</hibernate-mapping>
