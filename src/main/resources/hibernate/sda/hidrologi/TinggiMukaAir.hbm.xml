<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.integrasolusi.pusda.sisda.persistence.sda.hidrologi.TinggiMukaAir" table="hidrologi_tma">
        
        <id name="id" column="UID" type="java.lang.Long" unsaved-value="null">
            <generator class="native"/>
        </id>

        <version name="version" access="field" column="OBJ_VERSION"/>

        <many-to-one name="pos" class="com.integrasolusi.pusda.sisda.persistence.sda.hidrologi.PosTMA"
                     column="POS" lazy="false" not-null="true"/>


        <many-to-one name="year" class="com.integrasolusi.pusda.sisda.persistence.Year"
                     column="YEAR" lazy="false" not-null="true"/>


        <property name="filename" type="java.lang.String">
            <column name="FILENAME" sql-type="TEXT"/>
        </property>

        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" sql-type="TEXT"/>
        </property>

    </class>
</hibernate-mapping>
