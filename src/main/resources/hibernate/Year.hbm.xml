<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.integrasolusi.pusda.sisda.persistence.Year" table="t_year">

        <id name="id" column="UID" type="java.lang.Long" unsaved-value="null">
            <generator class="native"/>
        </id>

        <version name="version" access="field" column="OBJ_VERSION"/>

        <property name="value" type="java.lang.Integer" column="YEAR" unique="true" not-null="true"/>

    </class>
</hibernate-mapping>
