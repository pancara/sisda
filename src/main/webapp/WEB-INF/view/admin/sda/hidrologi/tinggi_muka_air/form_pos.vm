<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>
        <a href="#springUrl("/admin/sda/hidrologi/tinggi_muka_air/list.html")">Curah Hujan</a>
        &#187;
        <a href="#springUrl("/admin/sda/hidrologi/tinggi_muka_air/list/${ws.id}.html")">${ws.name}</a>
        &#187;
        <a href="#springUrl("/admin/sda/hidrologi/tinggi_muka_air/list/${ws.id}/${das.id}.html")">${das.name}</a>
        &#187;
        <span>Form Pos CH</span>

    </title>
</head>
<body>
<div id="tinggi_muka_air">
    <div>
        #springBind("form")
        #if ($status.getErrors().getErrorCount() > 0)
            <ul class='error'>
                #foreach($error in $status.errorMessages)
                    <li>$error</li>
                #end
            </ul>
        #end


        <form method="post" enctype="multipart/form-data">
            <table style='width: 95%;'>
                <col style="width: 120px;"/>
                <tr>
                    <td class="label">Nama</td>
                    <td>#springFormInput("form.name", "style='width: 400px;' class='text'")</td>
                </tr>
                <tr>
                    <td class="label">Keterangan</td>
                    <td>#springFormTextarea("form.description", "style='width: 400px;' class='text'")</td>
                </tr>

                <tr>
                    <td class="label">Longitude</td>
                    <td>#springFormInput("form.longitude", "style='width: 140px;' class='text'")</td>
                </tr>

                <tr>
                    <td class="label">Latitude</td>
                    <td>#springFormInput("form.latitude", "style='width: 140px;' class='text'")</td>
                </tr>
                <tr>
                    <td class="label">File</td>
                    <td><input type="file" name="file"/></td>
                </tr>
                <tr>
                    <td></td>
                    <td>
                        <input type="submit" value="submit" name='submit_action' class="button"/>
                    </td>
                </tr>
            </table>
        </form>
    </div>
</div>
</body>
</html>
