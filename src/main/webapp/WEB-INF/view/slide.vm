<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>
        Slide
    </title>
</head>
<body>
<div class="site_url_list">
    #foreach($url in $urls)
        <a href="${url.address}" title="${url.description}">
            <img src='#springUrl("/site_url/picture/${url.id}.html")' style="margin-right: 16px; max-width: 180px;"/>
        </a>
    #end
</div>
</body>
</html>
